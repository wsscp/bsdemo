package cc.oit.bsmes.common.constants;

public enum ProductOrderStatus {
	
	TO_DO("未开始"), IN_PROGRESS("生产中"), CANCELED("已取消"), FINISHED("已完成"), PAUSE("已暂停"),NOTQUALIFIED("不合格"),QUALIFIED("合格");

	private String status;

	private ProductOrderStatus(String status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return status;
	}
	
}
