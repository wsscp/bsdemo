package cc.oit.bsmes.test;

public class Person {
	private String code;
	private int age;

	public Person(String code, int age) {
		super();
		this.code = code;
		this.age = age;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	@Override
	public String toString() {
		StringBuffer sb = new StringBuffer();
		sb.append("{code:");
		sb.append(getCode());
		sb.append(", age:");
		sb.append(getAge());
		sb.append("}");
		return sb.toString();
	}
}
