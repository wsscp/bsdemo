.x-window-header-default-top {
    background-color: #287874;
    border-radius: 4px 4px 0 0;
    border-style: solid;
    border-width: 5px;
    padding: 5px;
}
.x-window-header-default {
    border-color: #287874;
    font-size: 13px;
}

.x-btn-default-small {
    background-color: #287874;
    background-image: -moz-linear-gradient(top,#88c6bc 0%,#59aca9 100%);
    background-image: -webkit-linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
    background-image: linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
    background-image: -ms-linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
}

.x-toolbar-footer {
    background: none repeat scroll 0 0 #dfeaf2;
    border: 0 none;
    margin: 0;
    padding: 6px 0 6px 6px;
}

.x-btn-default-small {
    border-color: #287874;
}

.x-window-default {
    border-color: #287874;
    border-radius: 4px;
}

/**
调整订单优先级设置
*/
.x-panel-default-framed {
    background-color: white;
    border-radius: 0px;
    border-style: solid;
    border-width: 0px;
    padding: 0;
}

.x-panel-header-default {
    background-color: #8fc5c3;
    background-image: none;
}

.x-panel-default-framed-outer-border-trbl {
    border-color: #8fc5c3 !important;
    border-width: 1px !important;
}

.x-border-layout-ct {
    background-color: #8fc5c3;
}

.x-btn-default-toolbar-small {
    background-color: #287874;
    background-image: -moz-linear-gradient(top,#88c6bc 0%,#59aca9 100%);
    background-image: -webkit-linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
    background-image: linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
    background-image: -ms-linear-gradient(top,  #88c6bc 0%,#59aca9 100%);
    border-radius: 3px;
    border-style: solid;
    border-width: 1px;
    padding: 3px;
}

.x-btn-default-toolbar-small .x-btn-inner {
    color: #ffffff;
}

.x-tab-bar-default {
    background-color: #287874;
}
.x-tab-default-top {
    background-color: #30918d;
}


.x-tab-default-active {
    background-color: #8fc5c3;
}

.x-tab-default-active .x-tab-inner {
    color: #FFFFFF;
}

.x-tab-bar {
    position: relative;
}