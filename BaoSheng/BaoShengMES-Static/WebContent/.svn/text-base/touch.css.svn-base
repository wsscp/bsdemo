/**
单元操作css样式
1、表格样式
*/
table.printtTable {
	border-collapse:collapse;
}
table.printtTable > tbody > tr > td {
	border:1px solid #000;
	text-align:center;
}
table.printtTable > tbody > tr > td.small {
	font-size: 12px;
}
table.printtTable > tbody > tr > td.title {
	width:57px;
	background-color:#93CDDD;
}
table.printtTable > tbody > tr > td.write {
	background-color: #FFA07A;
}
table.printtTable > tbody > tr > td.write > input {
	background-color: #FFA07A;
}

table.printtTable{
-moz-box-shadow: 3px 3px 0px #000000;
-webkit-box-shadow: 3px 3px 0px #000000;
box-shadow: 3px 3px 0px #000000;
}
table td input{
  border:0px ;
}
.table_box{
  margin:auto;
  margin-bottom:25px;
  border:1px solid #000;
  width:180mm;
}
.writing_tb{
    margin:0 auto;
	width:20px;
	line-height:24px;
}
.box1_w{
    width:190mm;
}
.box2_w{
    width:180mm;
}
.box3_w{
    width:170mm;
}
table.nav_table{
  border-collapse:collapse;
}
table.nav_table > tbody > tr > td{
  border:1px solid #000;
  text-align:center;
  padding:4px;
}
table.nav_table > tbody > tr > td.arrow-right{
  border:none;
  color:red;
  text-align:center;
}
table.nav_table > tbody > tr > td.small {
	font-size: 15px;
}
table > tbody > tr > td.imgBox {
	width:105px;
}
table > tbody > tr > td.imgBox > div {
	margin:auto;
	border:1px solid;
	width:95px;
	height:95px;
}
table > tbody > tr > td.imgBox > div  > img {
	width:100%;
	height:100%;
}
table > tbody > tr > td.center {
	text-align:center;
}

table > tbody > tr > td > input.needInput{
	background-color: #FFE4C4;
	width:100%;
	height:100%;
}
table > tbody > tr > td.tdInput{
	background-color: #FFE4C4;
}
