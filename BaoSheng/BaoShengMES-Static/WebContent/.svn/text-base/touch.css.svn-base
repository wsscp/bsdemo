/*radio css*/
.x-form-radio {
	background: url(icons/icr-green-skin.png) no-repeat;
	width: 23px;
	height: 23px;
	background-position: 1px -49px;
}

.x-form-radio:hover {
	background-position: -49px -49px;
}

.x-form-cb-checked .x-form-radio {
	background-position: -99px -49px;
}

.x-form-cb-checked:hover .x-form-radio {
	background-position: -149px -49px;
}

.x-form-checkbox {
	background: url(icons/icr-green-skin.png) no-repeat;
	width: 23px;
	height: 23px;
	background-position: 0 0;
}

.x-form-checkbox:hover {
	background-position: -50px 0;
}

.x-form-cb-checked .x-form-checkbox {
	background-position: -100px 0;
}

.x-form-cb-checked:hover .x-form-checkbox {
	background-position: -150px 0;
}

.x-form-cb {
	margin-top: 0px;
}

.x-form-trigger-wrap .x-form-text {
	border-width: 0;
	height: 50px;
}

